{
  "name": "yellow-book-nx",
  "private": true,
  "scripts": {
    "dev": "concurrently -k \"npm:dev:web\" \"npm:dev:api\"",
    "dev:web": "next dev apps/web",
    "dev:api": "ts-node -P apps/api/tsconfig.json -r tsconfig-paths/register --transpile-only apps/api/src/index.ts",
    "build": "next build apps/web",
    "start": "next start apps/web -p 3000",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc -p tsconfig.base.json --noEmit",
    "seed": "ts-node prisma/seed.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^3.0.3",
    "cookie-parser": "^1.4.7",
    "express": "^4.19.2",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.460.0",
    "next": "14.2.5",
    "next-auth": "^4.24.13",
    "openai": "^6.14.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nx/js": "^22.0.3",
    "@nx/workspace": "^22.0.3",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^24.10.1",
    "@types/react": "19.2.2",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "nx": "^22.0.3",
    "postcss": "^8.4.47",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  }
}
